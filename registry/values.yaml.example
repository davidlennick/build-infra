service:
  type: NodePort
  nodePort: 32000

secrets:
  htpasswd: |-
    <PW_HERE>
ingress:
  enabled: true
  hosts:
    - registry.lennick.ca
  annotations:
    kubernetes.io/ingress.class: nginx
    certmanager.k8s.io/cluster-issuer: letsencrypt-prod
    nginx.ingress.kubernetes.io/proxy-body-size: "30720m"
  tls:
    - secretName: letsencrypt-prod
      hosts:
        - registry.lennick.ca
